<p id="notice"><%= notice %></p>

<h1>Admin Tournaments Players</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_tournaments_players.each do |admin_tournaments_player| %>
      <tr>
        <td><%= link_to 'Show', admin_tournaments_player %></td>
        <td><%= link_to 'Edit', edit_admin_tournaments_player_path(admin_tournaments_player) %></td>
        <td><%= link_to 'Destroy', admin_tournaments_player, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

