<p id="notice"><%= notice %></p>

<h1>Admin Boards</h1>

<table>
  <thead>
    <tr>
      <th>Index</th>
      <th>Update</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_boards.each do |admin_board| %>
      <tr>
        <td><%= admin_board.index %></td>
        <td><%= admin_board.update %></td>
        <td><%= link_to 'Show', admin_board %></td>
        <td><%= link_to 'Edit', edit_admin_board_path(admin_board) %></td>
        <td><%= link_to 'Destroy', admin_board, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin Board', new_admin_board_path %>
