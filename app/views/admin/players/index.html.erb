<p id="notice"><%= notice %></p>

<h1>Admin Players</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_players.each do |player| %>
      <tr>
        <td><%= link_to 'Show', admin_player_path(player) %></td>
        <td><%= link_to 'Edit', edit_admin_player_path(player) %></td>
        <td><%= link_to 'Destroy', admin_player_path(player), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Add New Player', new_admin_player_path %>
