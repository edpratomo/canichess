<p id="notice"><%= notice %></p>

<h1>Admin Sponsors</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_sponsors.each do |admin_sponsor| %>
      <tr>
        <td><%= link_to 'Show', admin_sponsor %></td>
        <td><%= link_to 'Edit', edit_admin_sponsor_path(admin_sponsor) %></td>
        <td><%= link_to 'Destroy', admin_sponsor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin Sponsor', new_admin_sponsor_path %>
